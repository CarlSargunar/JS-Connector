{
  "name": "loop54-js-lib",
  "version": "1.0.0",
  "description": "JS lib for Loop54 easy frontend implementation.",
  "main": "lib/index.js",
  "scripts": {
    "dev": "node launch-dev.js",
    "watch": "watchify src/index.js -o lib/loop54-js-lib.js -t [ babelify --presets [ es2015 stage-2 ] ]",
    "server": "node server.js",
    "bundle": "browserify src/index.js -o lib/loop54-js-lib.js -t [ babelify --presets [ es2015 stage-2 ] ] | uglifyjs lib/loop54-js-lib.js -o lib/loop54-js-lib.min.js",
    "test": "mocha --require mocha-babel.js ./test/lib-test.js"
  },
  "test": "mocha --require mocha-babel.js ./test/lib-test.js",
  "keywords": [
    "Loop54",
    "SaaS",
    "Search"
  ],
  "author": "Loop54",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.7.0",
    "promise-polyfill": "^6.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-2": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^7.1.1",
    "express": "^4.16.2",
    "forever": "^0.15.3",
    "http": "0.0.0",
    "mocha": "^4.0.1",
    "nock": "^9.0.22",
    "npm-run-all": "^4.1.1",
    "sinon": "^4.0.1",
    "uglify-js": "^3.1.7",
    "watchify": "^3.9.0"
  }
}
